<div>
    <span class="has-text-grey">Reports edited outside of Analytics in last 6 months.</span>
    <div class="table-responsive">
        <table class="table sort" aria-label="edited outside analytics">
            <thead>
                <tr>
                    <th>Report Name</th>
                    <th>Report Type</th>
                    <th>Author</th>
                    <th>Last Modified By</th>
                    <th>Last Modified</th>
                    <th>Runs</th>
                    <th>RecordViewer</th>
                    @* <th>RunReport</th> *@
                    <th>Editor</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var t in ViewBag.EditedOutsideAnalytics)
                {
                    <tr>
                        <td><a href="@t.ReportUrl">@t.Name</a></td>
                        <td>@t.ReportType</td>
                        <td>@t.Author</td>
                        <td>@t.ModifiedBy</td>
                        <td>@t.LastMod</td>
                        <td>@t.Runs</td>
                        <td>
                            @if (t.RecordViewerUrl != null && t.RecordViewerUrl != "")
                            {
                                <a data-agl="AR_ITM_RECORDVIEWER" data-ini="@t.EpicMasterFile" data-id="@t.EpicRecordId" href="@t.RecordViewerUrl">@t.Epic</a>
                            } else
                            {
                                @t.Epic
                            }
                        </td>
                        @* <td>
                            @if (t.RunReportUrl != null && t.RunReportUrl != "")
                            {
                                <a href="@t.RunReportUrl">Run</a>
                            }
                        </td> *@
                        <td>
                            @if (t.EditReportUrl != null && t.EditReportUrl != "")
                            {
                                <a href="@t.EditReportUrl">Edit</a>
                            }
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
